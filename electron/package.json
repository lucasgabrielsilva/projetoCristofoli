{
  "name": "Cristofoli",
  "homepage": "https://www.cristofoli.com",
  "version": "1.0.0",
  "description": "Projeto de comunicação com a autoclave",
  "main": "./src/main.js",
  "author": "Cristófoli Biossegurança LTDA <lucas_gabrielsilva@hotmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "electron .",
    "rebuild": "electron-rebuild",
    "dist": "electron-builder --win --x64",
    "dist32": "electron-builder --win --ia32"
  },
  "devDependencies": {
    "electron": "^9.3.5",
    "electron-builder": "^22.8.0",
    "electron-rebuild": "^2.3.2"
  },
  "build": {
    "files": [
      "./build/**/*",
      "./src/**/*",
      "./assets/*",
      "node_modules/**/*"
    ],
    "win": {
      "appId": "com.electron.cristofoli.desktop",
      "target": "nsis",
      "icon": "./assets/cristofoli.ico"
    },
    "linux": {
      "appId": "com.electron.cristofoli.desktop",
      "target": "deb"
    }
  },
  "dependencies": {
    "axios": "^0.21.0",
    "basic-ftp": "^4.6.3",
    "fast-csv": "^4.3.2",
    "js-file-download": "^0.4.12",
    "moment": "^2.29.1",
    "promise-ftp": "^1.3.5",
    "serialport": "^9.0.2"
  }
}
